{
	"name": "@answeroverflow/db",
	"version": "0.1.0",
	"main": "./index.ts",
	"types": "./index.ts",
	"license": "MIT",
	"private": true,
	"scripts": {
		"clean": "rm -rf .turbo node_modules coverage",
		"with-env": "dotenv -e ../../.env --",
		"format": "prettier --ignore-path ../../.gitignore --config .prettierrc.cjs --write .",
		"format:check": "prettier --ignore-path ../../.gitignore --config .prettierrc.cjs --check .",
		"lint": "eslint . && tsc --noEmit",
		"lint:fix": "eslint . --fix",
		"db:push": "cross-env drizzle-kit push:mysql",
		"db:wipe": "cross-env pnpm with-env npx ts-node ./scripts/wipe.ts",
		"db:apply-last-indexed": "cross-env pnpm with-env npx ts-node ./scripts/apply-last-indexed.ts",
		"db:migrate-attachments": "cross-env pnpm with-env npx ts-node ./scripts/migrate-attachments.ts",
		"db:introspect": "drizzle-kit introspect:mysql",
		"test": "pnpm with-env vitest run --coverage",
		"test:watch": "pnpm with-env vitest",
		"dev": "echo http://localhost:4983 && pnpm drizzle-kit studio"
	},
	"dependencies": {
		"@answeroverflow/constants": "workspace:^",
		"@answeroverflow/db-mock": "workspace:^",
		"@answeroverflow/env": "workspace:^",
		"@answeroverflow/utils": "workspace:^",
		"@planetscale/database": "^1.11.0",
		"@sapphire/bitfield": "^1.2.0",
		"@vitest/coverage-c8": "^0.28.5",
		"aws-sdk": "^2.1565.0",
		"dataloader": "^2.2.2",
		"discord-api-types": "^0.37.67",
		"dotenv": "^16.4.5",
		"dotenv-cra": "^3.0.3",
		"drizzle-orm": "^0.28.5",
		"drizzle-zod": "^0.5.1",
		"json-bigint": "^1.0.0",
		"lodash": "^4.17.21",
		"mysql2": "3.6.0",
		"unique-names-generator": "^4.7.1",
		"uploadthing": "^6.1.0",
		"zod": "^3.22.2"
	},
	"devDependencies": {
		"@answeroverflow/prettier-config": "workspace:^",
		"@answeroverflow/tsconfig": "workspace:^",
		"@types/json-bigint": "^1.0.3",
		"@types/lodash": "^4.14.202",
		"@types/node": "^20.11.20",
		"cross-env": "^7.0.3",
		"dotenv-cli": "^7.3.0",
		"drizzle-kit": "^0.19.13",
		"eslint": "^8.47.0",
		"prettier": "^2.8.8",
		"ts-node": "^10.9.1",
		"vitest": "^0.29.8"
	}
}
