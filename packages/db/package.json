{
  "name": "@answeroverflow/db",
  "version": "0.1.0",
  "main": "./index.ts",
  "types": "./index.ts",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "init": "npx prisma migrate dev",
    "migrate": "npx prisma migrate deploy",
    "with-env": "dotenv -e ../../.env --",
    "dev": "yarn with-env prisma studio --port 5556 --browser none",
    "lint": "eslint \"*.ts*\"",
    "lint:fix": "eslint \"*.ts*\" --fix",
    "test": "vitest run --no-threads",
    "test:watch": "vitest --no-threads",
    "db-push": "yarn with-env prisma db push",
    "db-generate": "yarn with-env prisma generate"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.5.0",
    "@prisma/client": "^4.7.1"
  },
  "devDependencies": {
    "@answeroverflow/tsconfig": "*",
    "@vitest/coverage-c8": "^0.25.8",
    "dotenv-cli": "^6.0.0",
    "prisma": "^4.7.1",
    "typescript": "^4.9.3",
    "vitest": "^0.25.8"
  }
}
