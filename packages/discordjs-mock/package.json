{
  "name": "@answeroverflow/discordjs-mock",
  "version": "0.0.12",
  "main": "index.ts",
  "types": "index.ts",
  "dist-main": "./dist/index.js",
  "dist-types": "./dist/index.d.ts",
  "type": "module",
  "author": "@answeroverflow",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": "{\n  \".\": {\n    \"import\": \"./dist/index.js\",\n    \"require\": \"./dist/index.cjs\",\n    \"types\": \"./dist/index.d.ts\"\n  },\n  \"./package.json\": {\n    \"import\": \"./package.json\",\n    \"require\": \"./package.json\"\n  }\n}",
  "devDependencies": {
    "@answeroverflow/discordjs-utils": "*",
    "@answeroverflow/utils": "*",
    "@swc/core": "^1.3.32",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.2",
    "jest": "^29.4.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsup": "^6.6.3"
  },
  "scripts": {
    "prepublishOnly": "npx main-updater@latest prepublish && yarn build",
    "postpublish": "npx main-updater@latest postpublish",
    "build": "tsup index.ts --target node16 --format cjs,esm --dts --sourcemap",
    "build-watch": "yarn build -- --watch",
    "clean": "rm -rf dist node_modules turbo coverage",
    "test": "yarn jest --coverage",
    "test:watch": "yarn jest --watch",
    "lint": "eslint . && tsc --noEmit",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {
    "discord.js": "^14.7.1"
  },
  "resolutions": {
    "discord-api-types": "0.37.20"
  }
}
