{
	"name": "@answeroverflow/discordjs-mock",
	"version": "0.0.12",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"author": "@answeroverflow",
	"license": "MIT",
	"files": ["dist"],
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"types": "./dist/index.d.ts"
		},
		"./package.json": {
			"import": "./package.json",
			"require": "./package.json"
		}
	},
	"devDependencies": {
		"@answeroverflow/discordjs-utils": "*",
		"@answeroverflow/utils": "*",
		"@swc/core": "^1.3.32",
		"@swc/jest": "^0.2.24",
		"@types/jest": "^29.5.0",
		"@types/node": "^18.14.2",
		"jest": "^29.5.0",
		"ts-jest": "^29.1.0",
		"ts-node": "^10.9.1",
		"tsup": "^6.6.3"
	},
	"scripts": {
		"prepublishOnly": "yarn build",
		"build": "tsup index.ts --target node16 --format cjs,esm --dts --sourcemap",
		"build-watch": "yarn build -- --watch",
		"clean": "rm -rf dist node_modules turbo coverage",
		"test": "yarn jest --coverage",
		"test:watch": "yarn jest --watch",
		"format": "prettier --ignore-path ../../.gitignore --config ./prettierrc.cjs --write .",
		"format:check": "prettier --ignore-path ../../.gitignore --config ./prettierrc.cjs --check .",
		"lint": "eslint . && tsc --noEmit",
		"lint:fix": "eslint . --fix"
	},
	"peerDependencies": {
		"discord.js": "^14.7.1"
	},
	"dependencies": {
		"discord.js": "^14.8.0"
	}
}
