{
  "name": "@answeroverflow/discordjs-mock",
  "version": "0.0.2",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "author": "@answeroverflow",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": {
      "import": "./package.json",
      "require": "./package.json"
    }
  },
  "devDependencies": {
    "@answeroverflow/discordjs-utils": "*",
    "@answeroverflow/utils": "*",
    "@swc/core": "^1.3.32",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.2",
    "jest": "^29.4.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsup": "^6.6.3"
  },
  "scripts": {
    "prepublish": "yarn build",
    "build": "tsup index.ts --target node16 --format cjs,esm --dts --sourcemap",
    "build-watch": "yarn build -- --watch",
    "clean": "rm -rf dist node_modules turbo coverage",
    "test": "yarn jest --coverage",
    "test:watch": "yarn jest --watch",
    "lint": "eslint . && tsc --noEmit",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {
    "discord.js": "^14.7.1"
  },
  "resolutions": {
    "discord-api-types": "0.37.20"
  }
}
