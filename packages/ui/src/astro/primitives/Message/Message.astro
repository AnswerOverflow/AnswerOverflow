---
import type { MessageProps } from './Props';
import MessageAuthor from "./MessageAuthor.astro"
type Props = MessageProps
const props = Astro.props;
---
{
  !props.hidden && (
    <div>
			<MessageAuthor {...props} />

     	<!-- Content -->
     	<slot name="text" />
        meow
     	<slot name="embeds" />
     	<slot name="attachments" />

     	<slot name="footer" />
    </div>
  )
}
