{
	"name": "@answeroverflow/auth",
	"version": "0.1.0",
	"main": "./index.ts",
	"types": "./index.ts",
	"license": "MIT",
	"private": true,
	"scripts": {
		"clean": "rm -rf .turbo node_modules coverage",
		"format": "prettier --ignore-path ../../.gitignore --config .prettierrc.cjs --write .",
		"format:check": "prettier --ignore-path ../../.gitignore --config .prettierrc.cjs --check .",
		"lint": "eslint . --ext .ts && tsc --noEmit",
		"lint:fix": "eslint . --ext .ts --fix",
		"with-env": "dotenv -e ../../.env --",
		"test": "pnpm with-env vitest run --coverage",
		"test:watch": "pnpm with-env vitest",
		"type-check": "tsc --noEmit"
	},
	"dependencies": {
		"@answeroverflow/analytics": "workspace:^",
		"@answeroverflow/cache": "workspace:^",
		"@answeroverflow/db": "workspace:^",
		"@answeroverflow/constants": "workspace:^",
		"@answeroverflow/env": "workspace:^",
		"@answeroverflow/utils": "workspace:^",
		"@auth/core": "^0.12.0",
		"dotenv-cra": "^3.0.3",
		"drizzle-orm": "^0.28.5",
		"next": "14.1.0",
		"next-auth": "^4.23.1",
		"prettier": "^2.8.8",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"zod": "^3.22.2"
	},
	"devDependencies": {
		"@answeroverflow/prettier-config": "workspace:^",
		"@answeroverflow/tsconfig": "workspace:^",
		"@types/cookie": "^0.5.1",
		"@types/node": "^20.11.20",
		"@vitest/coverage-c8": "^0.28.5",
		"dotenv-cli": "^7.3.0",
		"eslint": "^8.57.0",
		"vitest": "^0.29.8"
	}
}
