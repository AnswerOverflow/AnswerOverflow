	"license": "MIT",
	"private": true,
	"scripts": {
		"with-env": "dotenv -e ../../.env --",
		"clean": "rm -rf .turbo node_modules coverage",
		"format": "prettier --ignore-path ../../.gitignore --config .prettierrc.cjs --write .",
		"format:check": "prettier --ignore-path ../../.gitignore --config .prettierrc.cjs --check .",
		"lint": "eslint . && tsc --noEmit",
		"lint:fix": "eslint . --fix",
		"test": "pnpm with-env vitest run --coverage",
		"test:watch": "pnpm with-env vitest",
		"type-check": "tsc --noEmit"
	},
	"dependencies": {
		"@answeroverflow/analytics": "workspace:^",
		"@answeroverflow/auth": "workspace:^",
		"@answeroverflow/cache": "workspace:^",
		"@answeroverflow/constants": "workspace:^",
		"@answeroverflow/db": "workspace:^",
		"@answeroverflow/discordjs-utils": "workspace:^",
		"@answeroverflow/env": "workspace:^",
		"@answeroverflow/payments": "workspace:^",
		"@answeroverflow/search": "workspace:^",
		"@answeroverflow/utils": "workspace:^",
		"@sapphire/bitfield": "^1.2.0",
		"@trpc/server": "^10.37.1",
		"@types/qs": "^6.9.7",
		"dayjs": "^1.11.9",
		"discord-api-types": "^0.37.67",
		"dotenv-cra": "^3.0.3",
		"next": "14.1.4",
		"prettier": "^2.8.8",
		"qs": "^6.11.2",
		"stripe": "^12.18.0",
