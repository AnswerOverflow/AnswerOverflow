  "devDependencies": {
    "@answeroverflow/discordjs-mock": "*",
    "@answeroverflow/eslint-config-custom": "*",
    "@answeroverflow/tsconfig": "*",
    "@sapphire/eslint-config": "^4.3.8",
    "@swc/core": "^1.3.22",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.14.2",
    "@types/ws": "^8.5.3",
    "dotenv-cli": "^7.0.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "tsup": "^6.3.0",
    "type-fest": "^3.5.7",
    "typescript": "^4.9.4"
  },
  "scripts": {
    "with-env": "dotenv -e ../../.env --",
    "clean": "rm -rf .turbo node_modules dist coverage",
    "watch": "tsup  --silent --format cjs --watch . --watch ../../packages/api/src/",
    "build": "tsup --format cjs",
    "lint": "eslint . && tsc --noEmit",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "dev": "yarn run with-env rm -rf dist && yarn run build && run-p watch start",
    "start": "yarn run with-env node dist/index.js",
    "test": "yarn run with-env jest --coverage",
    "test:watch": "yarn run with-env jest --watch",
    "install-local-reacord": "cp -r ~/repos/reacord/packages/reacord/dist/ /workspace/node_modules/@answeroverflow/reacord/"
  },
  "resolutions": {
    "discord-api-types": "0.37.20"
  }
}
