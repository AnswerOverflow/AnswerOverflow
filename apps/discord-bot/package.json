	"name": "@answeroverflow/discord-bot",
	"version": "1.0.0",
	"main": "dist/index.js",
	"author": "@answeroverflow",
	"license": "UNLICENSE",
	"dependencies": {
		"@answeroverflow/api": "*",
		"@answeroverflow/cache": "*",
		"@answeroverflow/constants": "*",
		"@answeroverflow/db": "*",
		"@answeroverflow/db-mock": "*",
		"@answeroverflow/discordjs-react": "^0.0.35",
		"@answeroverflow/discordjs-utils": "*",
		"@sapphire/decorators": "6.0.1",
		"@sapphire/discord-utilities": "3.0.3",
		"@sapphire/fetch": "^2.4.1",
		"@sapphire/framework": "^4.2.2",
		"@sapphire/plugin-api": "5.0.2",
		"@sapphire/plugin-editable-commands": "3.0.0",
		"@sapphire/plugin-hmr": "^2.0.0",
		"@sapphire/plugin-logger": "^3.0.1",
		"@sapphire/plugin-subcommands": "^4.0.0",
		"@sapphire/time-utilities": "^1.7.8",
		"@sapphire/type": "^2.2.4",
		"@sapphire/utilities": "^3.10.1",
		"@sentry/node": "^7.43.0",
		"@sentry/tracing": "^7.47.0",
		"@trpc/server": "^10.25.0",
		"colorette": "^2.0.19",
		"discord-api-types": "^0.37.20",
		"discord.js": "^14.8.0",
		"dotenv-cra": "^3.0.2",
		"lru-cache": "^8.0.4",
		"react": "^18.2.0",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.8.1",
		"tsx": "^3.12.1"
	},
	"devDependencies": {
		"@answeroverflow/discordjs-mock": "*",
		"@answeroverflow/eslint-config-custom": "*",
		"@answeroverflow/tsconfig": "*",
		"@sapphire/eslint-config": "^4.4.1",
		"@swc/core": "^1.3.22",
		"@swc/jest": "^0.2.24",
		"@types/jest": "^29.5.0",
		"@types/node": "^18.14.2",
		"@types/react": "^18.0.28",
		"@types/ws": "^8.5.3",
		"dotenv-cli": "^7.1.0",
		"eslint-plugin-jest": "^27.2.1",
		"jest": "^29.5.0",
		"npm-run-all": "^4.1.5",
		"ts-jest": "^29.1.0",
		"ts-node": "^10.9.1",
		"tsc-watch": "^5.0.3",
		"tsup": "^6.3.0",
		"type-fest": "^3.5.7"
	},
	"scripts": {
		"with-env": "dotenv -e ../../.env --",
		"clean": "rm -rf .turbo node_modules dist coverage",
		"watch": "tsup  --silent --format cjs --watch . --watch ../../packages/api/src/",
		"format": "prettier --ignore-path ../../.gitignore --config ./prettierrc.cjs --write .",
		"format:check": "prettier --ignore-path ../../.gitignore --config ./prettierrc.cjs --check .",
		"lint": "eslint . && tsc --noEmit",
		"lint:fix": "eslint . --fix",
		"build-deps": "(cd ../../packages/discordjs-mock && yarn build)",
		"build": "yarn build-deps && tsup --format cjs",
		"dev": "yarn build-deps && yarn run with-env rm -rf dist && yarn run build && run-p watch start",
		"start": "yarn run with-env node dist/index.js",
		"test": "yarn build-deps && yarn run with-env jest --coverage",
		"test:watch": "yarn run with-env jest --watch",
		"install-local-discordjs-react": "cp -r ~/repos/discordjs-react/packages/discordjs-react/dist/ /AnswerOverflow/node_modules/@answeroverflow/discordjs-react/"
	}
}
