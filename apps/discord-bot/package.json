    "@sapphire/fetch": "^2.4.1",
    "@sapphire/framework": "4.0.0",
    "@sapphire/plugin-api": "5.0.1",
    "@sapphire/plugin-editable-commands": "3.0.0",
    "@sapphire/plugin-hmr": "^2.0.0",
    "@sapphire/plugin-logger": "^3.0.1",
    "@sapphire/plugin-subcommands": "^3.2.2",
    "@sapphire/time-utilities": "^1.7.8",
    "@sapphire/type": "^2.2.4",
    "@sapphire/utilities": "^3.10.1",
    "@trpc/server": "^10.4.3",
    "colorette": "^2.0.19",
    "discord-api-types": "^0.37.20",
