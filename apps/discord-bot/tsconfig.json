{
	"extends": "@sapphire/ts-config",
	"compilerOptions": {
		"rootDir": "src",
		"outDir": "dist",
		"tsBuildInfoFile": "dist/.tsbuildinfo",
		"noUnusedLocals": true,
		"noUnusedParameters": false,		
		"skipLibCheck": true,		
		"target": "ES2017",
		"module": "esnext"

	},
	"include": ["src"],
	"exclude": ["node_modules"]
}
