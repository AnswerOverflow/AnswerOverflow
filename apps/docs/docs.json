{
	"$schema": "https://mintlify.com/docs.json",
	"theme": "mint",
	"name": "Answer Overflow",
	"logo": {
		"dark": "https://www.answeroverflow.com/AnswerOverflowLogoLight.png",
		"light": "https://www.answeroverflow.com/AnswerOverflowLogoDark.png"
	},
	"colors": {
		"primary": "#88c7e4",
		"light": "#7dabd2",
		"dark": "#79a6cc"
	},
	"favicon": "/favicon-32x32.png",

	"navigation": {
		"tabs": [
			{
				"tab": "Docs",
				"groups": [
					{
						"group": "Essentials",
						"pages": ["overview", "quick-start", "recommended-setup"]
					},
					{
						"group": "Channel Settings",
						"pages": [
							"channel-settings/auto-thread",
							"channel-settings/forum-guidelines-consent",
							"channel-settings/indexing",
							"channel-settings/mark-solution",
							"channel-settings/send-mark-solution-instructions",
							"channel-settings/solved-tag"
						]
					},
					{
						"group": "Server Settings",
						"pages": ["server-settings/read-the-rules-consent"]
					},
					{
						"group": "User Settings",
						"pages": [
							"user-settings/displaying-messages",
							"user-settings/globally-ignore-account",
							"user-settings/ignore-account-in-server"
						]
					},
					{
						"group": "Integrations",
						"pages": ["integrations/inkeep"]
					},
					{
						"group": "Changelog",
						"pages": [
							"changelog/solved-posts-now-auto-archive",
							"changelog/introducing-the-answer-overflow-public-platform",
							"changelog/app-router",
							"changelog/answeroverflow-now-has-a-changelog"
						]
					}
				]
			},
			{
				"tab": "API Reference",
				"openapi": "https://www.answeroverflow.com/api/openapi-mintlify"
			}
		],
		"global": {
			"anchors": [
				{
					"anchor": "Community",
					"href": "https://discord.answeroverflow.com",
					"icon": "discord"
				}
			]
		}
	},
	"navbar": {
		"links": [
			{
				"label": "Get in touch",
				"href": "mailto:rhys@answeroverflow.com"
			}
		],
		"primary": {
			"type": "button",
			"label": "Setup for free",
			"href": "https://www.answeroverflow.com/dashboard"
		}
	},
	"footer": {
		"socials": {
			"x": "https://x.com/rhyssullivan",
			"github": "https://github.com/answeroverflow"
		}
	}
}
