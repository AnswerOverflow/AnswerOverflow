{
	"name": "@answeroverflow/dashboard",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"with-env": "dotenv -e ../../.env --",
		"dev": "pnpm with-env next dev --port 5000",
		"build": "pnpm with-env next build",
		"clean": "rm -rf .next .turbo node_modules",
		"start": "pnpm with-env next start --port 5000",
		"format": "prettier --ignore-path ../../.gitignore --config .prettierrc.cjs --write .",
		"format:check": "prettier --ignore-path ../../.gitignore --config .prettierrc.cjs --check .",
		"lint": "pnpm with-env next lint && tsc --noEmit",
		"lint:fix": "pnpm with-env next lint --fix",
		"type-check": "tsc --noEmit"
	},
	"dependencies": {
		"@answeroverflow/api": "workspace:^",
		"@answeroverflow/auth": "workspace:^",
		"@answeroverflow/constants": "workspace:^",
		"@answeroverflow/db": "workspace:^",
		"@answeroverflow/env": "workspace:^",
		"@answeroverflow/hooks": "workspace:^",
		"@answeroverflow/tailwind-config": "workspace:^",
		"@answeroverflow/ui": "workspace:^",
		"@heroicons/react": "^2.0.18",
		"@tanstack/react-query": "^4.36.1",
		"@tremor/react": "^3.6.1",
		"@trpc/server": "^10.45.2",
		"next": "14.3.0-canary.9",
		"next-auth": "^4.23.1",
		"posthog-js": "^1.83.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-icons": "^4.10.1",
		"react-toastify": "^10.0.5",
		"remark-gfm": "^3.0.1",
		"sharp": "^0.32.5",
		"stripe": "^12.18.0",
		"zod": "^3.22.2"
	},
	"devDependencies": {
		"@answeroverflow/eslint-config-custom": "workspace:^",
		"@answeroverflow/prettier-config": "workspace:^",
		"@answeroverflow/tsconfig": "workspace:^",
		"@edge-runtime/vm": "^2.1.2",
		"@headlessui/tailwindcss": "^0.2.0",
		"@types/node": "^20.11.20",
		"@types/react": "^18.2.20",
		"@types/react-dom": "^18.2.7",
		"@vitest/coverage-c8": "^0.30.1",
		"autoprefixer": "^10.4.15",
		"dotenv-cli": "^7.4.1",
		"eslint": "^8.57.0",
		"eslint-config-next": "14.1.3",
		"postcss": "^8.4.28",
		"prettier": "^2.8.8",
		"sass": "^1.66.1",
		"tailwindcss": "^3.3.3",
		"vitest": "^0.30.1"
	}
}
