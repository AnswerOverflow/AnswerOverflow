{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": "../../",
    "types": [
      "vitest/globals",
      "node"
    ],
    "strictNullChecks": true,
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      },
    ],
  },
  "include": [
    "src",
    "uno.config.ts",
    "../../reset.d.ts",
    "vitest.config.ts",
    ".prettierrc.mjs",
  ],
  "exclude": [
    "node_modules"
  ]
}
