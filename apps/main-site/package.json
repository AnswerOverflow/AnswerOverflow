{
	"name": "@answeroverflow/main-site",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"with-env": "dotenv -e ../../.env --",
		"dev": "pnpm with-env next dev",
		"build": "pnpm with-env next build",
		"clean": "rm -rf .next .turbo node_modules",
		"start": "pnpm with-env next start",
		"format": "prettier --ignore-path ../../.gitignore --config .prettierrc.cjs --write .",
		"format:check": "prettier --ignore-path ../../.gitignore --config .prettierrc.cjs --check .",
		"lint": "pnpm with-env next lint && tsc --noEmit",
		"lint:fix": "pnpm with-env next lint --fix",
		"type-check": "tsc --noEmit"
	},
	"dependencies": {
		"@answeroverflow/analytics": "workspace:^",
		"@answeroverflow/api": "workspace:^",
		"@answeroverflow/auth": "workspace:^",
		"@answeroverflow/constants": "workspace:^",
		"@answeroverflow/db": "workspace:^",
		"@answeroverflow/env": "workspace:^",
		"@answeroverflow/hooks": "workspace:^",
		"@answeroverflow/payments": "workspace:^",
		"@answeroverflow/search": "workspace:^",
		"@answeroverflow/tailwind-config": "workspace:^",
		"@answeroverflow/ui": "workspace:^",
		"@badrap/bar-of-progress": "^0.2.2",
		"@mdx-js/loader": "^2.3.0",
		"@mdx-js/react": "^2.3.0",
		"@next/mdx": "^13.4.19",
		"@sentry/nextjs": "^7.72.0",
		"@tanstack/react-query": "^4.32.6",
		"@tanstack/react-query-devtools": "^4.32.6",
		"@tremor/react": "^3.6.1",
		"@trpc/client": "^10.37.1",
		"@trpc/next": "^10.37.1",
		"@trpc/react-query": "^10.37.1",
		"@trpc/server": "^10.37.1",
		"@vercel/og": "^0.5.13",
		"core-js": "^3.32.1",
		"drizzle-orm": "^0.28.5",
		"highlight.js": "^11.8.0",
		"next": "13.5.6",
		"next-auth": "^4.23.1",
		"next-themes": "^0.2.1",
		"next13-progressbar": "^1.0.3",
		"nextjs-google-analytics": "^2.3.3",
		"raw-body": "^2.5.2",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-icons": "^4.10.1",
