{
	"name": "@answeroverflow/main-site",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"with-env": "dotenv -e ../../.env --",
		"dev": "pnpm with-env next dev",
		"build": "(cd ../../ && pnpm db:generate && cd apps/main-site) && pnpm with-env next build",
		"clean": "rm -rf .next .turbo node_modules",
		"start": "pnpm with-env next start",
		"format": "prettier --ignore-path ../../.gitignore --config .prettierrc.cjs --write .",
		"format:check": "prettier --ignore-path ../../.gitignore --config .prettierrc.cjs --check .",
		"lint": "pnpm with-env next lint && tsc --noEmit",
		"lint:fix": "pnpm with-env next lint --fix",
		"type-check": "tsc --noEmit"
	},
	"dependencies": {
		"@answeroverflow/analytics": "^0.1.0",
		"@answeroverflow/api": "^0.1.0",
		"@answeroverflow/auth": "^0.1.0",
		"@answeroverflow/constants": "^0.1.0",
		"@answeroverflow/db": "^0.1.0",
		"@answeroverflow/env": "^0.1.0",
		"@answeroverflow/hooks": "^0.1.0",
		"@answeroverflow/payments": "^0.1.0",
		"@answeroverflow/tailwind-config": "^0.1.0",
		"@answeroverflow/ui": "^0.1.0",
		"@badrap/bar-of-progress": "^0.2.2",
		"@mdx-js/loader": "^2.3.0",
		"@mdx-js/react": "^2.3.0",
		"@next/font": "^13.4.10",
		"@next/mdx": "^13.4.10",
		"@sentry/nextjs": "^7.58.1",
		"@tanstack/react-query": "^4.29.23",
		"@tanstack/react-query-devtools": "^4.29.23",
		"@tremor/react": "^3.4.1",
		"@trpc/client": "^10.34.0",
		"@trpc/next": "^10.34.0",
		"@trpc/react-query": "^10.34.0",
		"@trpc/server": "^10.34.0",
		"core-js": "^3.31.1",
		"highlight.js": "^11.8.0",
		"next": "13.4.8",
		"next-auth": "^4.22.1",
		"next-themes": "^0.2.1",
		"nextjs-google-analytics": "^2.3.3",
		"raw-body": "^2.5.2",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-toastify": "^9.1.3",
		"remark-gfm": "^3.0.1",
		"sharp": "^0.32.3",
		"stripe": "^12.13.0",
		"superjson": "^1.12.4",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@answeroverflow/eslint-config-custom": "^0.0.0",
		"@answeroverflow/prettier-config": "^0.0.0",
		"@answeroverflow/tsconfig": "^0.0.0",
		"@edge-runtime/vm": "^2.1.2",
		"@types/core-js": "^2.5.5",
		"@types/node": "^18.16.19",
		"@types/react": "^18.2.15",
		"@types/react-dom": "^18.2.7",
		"@vitest/coverage-c8": "^0.30.1",
		"autoprefixer": "^10.4.14",
		"dotenv-cli": "^7.2.1",
		"eslint": "^8.45.0",
		"eslint-config-next": "13.4.9",
		"postcss": "^8.4.26",
		"prettier": "^2.8.8",
		"sass": "^1.63.6",
		"tailwindcss": "^3.3.3",
		"vitest": "^0.30.1"
	}
