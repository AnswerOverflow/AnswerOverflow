{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [
    ".env.local"
  ],
  "ui": "tui",
  "globalEnv": [
    "PORT",
    "VERCEL_URL",
    "NODE_ENV",
    "DISCORD_TOKEN",
    "NEXT_PUBLIC_VERCEL_GIT_COMMIT_SHA",
    "DEFAULT_DELAY_IN_MS",
    "DATABASE_URL",
    "ELASTICSEARCH_URL",
    "ELASTICSEARCH_PASSWORD",
    "ELASTICSEARCH_USERNAME",
    "ELASTICSEARCH_MESSAGE_INDEX",
    "REDIS_URL",
    "ELASTICSEARCH_CLOUD_ID",
    "NEXT_PUBLIC_GA_MEASUREMENT_ID",
    "DISCORD_CLIENT_ID",
    "DISCORD_CLIENT_SECRET",
    "SKIP_ENV_VALIDATION",
    "CI",
    "INDEXING_INTERVAL_IN_HOURS",
    "MAXIMUM_CHANNEL_MESSAGES_PER_INDEX",
    "THEME",
    "NEXT_PUBLIC_DEPLOYMENT_ENV",
    "INDEXING_DISABLED",
    "MAX_NUMBER_OF_THREADS_TO_COLLECT",
    "NEXT_PUBLIC_POSTHOG_TOKEN",
    "STATUS_UPDATE_INTERVAL_IN_HOURS",
    "PRINT_COMMUNITIES",
    "MAX_NUMBER_OF_MESSAGES_TO_COLLECT",
    "STRIPE_PRO_PLAN_PRICE_ID",
    "STRIPE_PAGE_VIEWS_PRICE_ID",
    "STRIPE_SECRET_KEY",
    "STRIPE_WEBHOOK_SECRET",
    "NEXT_PUBLIC_SITE_URL",
    "POSTHOG_PERSONAL_API_KEY",
    "POSTHOG_PROJECT_ID",
    "LADLE",
    "ENVIRONMENT",
    "NEXT_PUBLIC_SENTRY_DSN",
    "STRIPE_CHECKOUT_URL",
    "STRIPE_ENTERPRISE_PLAN_PRICE_ID",
    "NEXT_PUBLIC_SENTRY_DSN",
    "VERCEL_GIT_COMMIT_SHA",
    "AXIOM_API_KEY",
    "PROJECT_ID_VERCEL",
    "AUTH_BEARER_TOKEN_VERCEL",
    "TEAM_ID_VERCEL",
    "BUCKET_NAME",
    "IAM_USER_KEY",
    "IAM_USER_SECRET"
  ],
  "tasks": {
    "init": {},
    "migrate": {},
    "build": {
      "outputs": [
        "dist/**",
        ".next/**"
      ],
      "cache": true,
      "inputs": [
        "src/**/*.tsx",
        "src/**/*.ts",
        "test/**/*.ts"
      ]
    },
    "format": {
      "inputs": [
        "./packages/config/prettier/**/*",
        "src/**/*.tsx",
        "src/**/*.ts",
        "test/**/*.ts"
      ],
      "outputs": [],
      "cache": false
    },
    "format:check": {
      "outputs": [],
      "cache": true
    },
    "lint": {
      "inputs": [
        "./packages/config/eslint-config-custom/**/*",
        "src/**/*.tsx",
        "src/**/*.ts",
        "test/**/*.ts"
      ],
      "outputs": [],
      "cache": true
    },
    "lint:fix": {
      "outputs": [],
      "cache": false
    },
    "test": {
      "outputs": [
        "coverage/**"
      ],
      "cache": false,
      "inputs": [
        "src/**/*.tsx",
        "src/**/*.ts",
        "test/**/*.ts"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "db:wipe": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:push:staging": {
      "cache": false
    },
    "db:introspect": {
      "cache": false
    }
  }
}
