{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  },
  "globalDependencies": [
    ".env.local"
  ],
  "globalEnv": [
    "NODE_ENV",
    "DISCORD_TOKEN",
    "OWNERS",
    "VERCEL_URL",
    "PORT",
    "DATABASE_URL",
    "ELASTIC_IP",
    "ELASTIC_USERNAME",
    "ELASTIC_PASSWORD",
    "ELASTIC_API_KEY"
  ]
}