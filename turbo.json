{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  },
  "globalDependencies": [
    ".env.local"
  ],
  "globalEnv": [
    "NODE_ENV",
    "DISCORD_TOKEN",
    "OWNERS"
  ]
}
