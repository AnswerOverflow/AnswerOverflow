{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ],
      "cache": false
    },
    "init": {},
    "migrate": {},
    "lint": {
      "outputs": [],
      "cache": false
    },
    "lint:fix": {
      "outputs": [],
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "test": {
      "dependsOn": [
        "test:discord-bot"
      ],
      "outputs": [],
      "cache": false
    },
    "test:db": {
      "dependsOn": [
        "^test:db"
      ],
      "cache": false
    },
    "test:auth": {
      "dependsOn": [
        "test:db",
        "^test:auth"
      ],
      "cache": false
    },
    "test:api": {
      "dependsOn": [
        "test:auth",
        "^test:api"
      ],
      "cache": false
    },
    "test:discord-bot": {
      "dependsOn": [
        "test:api",
        "^test:discord-bot"
      ],
      "cache": false
    },
    "test:watch": {
      "outputs": [],
      "cache": false
    },
    "db:generate": {
      "inputs": [
        "prisma/schema.prisma"
      ],
      "cache": false
    },
    "db:push": {
      "inputs": [
        "prisma/schema.prisma"
      ],
      "cache": false
    }
  },
  "globalDependencies": [
    ".env.local"
  ],
  "globalEnv": [
    "NODE_ENV",
    "DISCORD_TOKEN",
    "OWNERS",
    "VERCEL_URL",
    "PORT",
    "DATABASE_URL",
    "ELASTICSEARCH_URL",
    "ELASTICSEARCH_PASSWORD",
    "ELASTICSEARCH_USERNAME",
    "VITE_ELASTICSEARCH_URL",
    "VITE_ELASTICSEARCH_PASSWORD",
    "VITE_ELASTICSEARCH_USERNAME",
    "ELASTICSEARCH_MESSAGE_INDEX",
    "VITE_ELASTICSEARCH_MESSAGE_INDEX",
    "ELASTICSEARCH_CLOUD_ID",
    "ELASTICSEARCH_API_KEY",
    "VITE_DATABASE_URL",
    "DISCORD_CLIENT_ID",
    "DISCORD_CLIENT_SECRET",
    "SKIP_ENV_VALIDATION",
    "VITE_DISCORD_CLIENT_ID",
    "CI",
    "INDEXING_INTERVAL_IN_HOURS",
    "BOT_DEV_LOG_LEVEL",
    "BOT_TEST_LOG_LEVEL",
    "BOT_PROD_LOG_LEVEL",
    "MAXIMUM_CHANNEL_MESSAGES_PER_INDEX",
    "STORYBOOK",
    "VITEST_DISCORD_CLIENT_ID"
  ]
}
