{
  "name": "@answeroverflow/core",
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "exports": {
    "./*": [
      "./src/*/index.ts",
      "./src/*.ts"
    ],
    "./mock": "./mock.ts"
  },
  "scripts": {
    "clean": "rm -rf .turbo node_modules coverage",
    "with-env": "dotenv -e ../../.env --",
    "format": "prettier --ignore-path ../../.gitignore --config .prettierrc.mjs --write .",
    "format:check": "prettier --ignore-path ../../.gitignore --config .prettierrc.mjs --check .",
    "lint": "eslint . && tsc --noEmit",
    "lint:fix": "eslint . --fix",
    "db:push": "drizzle-kit push:mysql",
    "db:wipe": "bun ./scripts/wipe.ts",
    "db:seed": "bun db:push && bun ./scripts/seed.ts",
    "db:introspect": "drizzle-kit introspect:mysql",
    "dev": "echo http://localhost:4983 && bun drizzle-kit studio"
  },
  "dependencies": {
    "@answeroverflow/constants": "workspace:*",
    "@answeroverflow/utils": "workspace:*",
    "@aws-sdk/client-s3": "^3.577.0",
    "@aws-sdk/lib-storage": "^3.578.0",
    "@planetscale/database": "^1.11.0",
    "@sapphire/bitfield": "^1.2.0",
    "dataloader": "^2.2.2",
    "discord-api-types": "^0.37.67",
    "dotenv": "^16.4.5",
    "dotenv-cra": "^3.0.3",
    "drizzle-orm": "^0.28.5",
    "drizzle-zod": "^0.5.1",
    "json-bigint": "^1.0.0",
    "lodash": "^4.17.21",
    "mysql2": "3.9.8",
    "unique-names-generator": "^4.7.1",
    "uploadthing": "^6.1.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@answeroverflow/tsconfig": "workspace:*",
    "@types/json-bigint": "^1.0.3",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.20",
    "dotenv-cli": "^7.4.1",
    "drizzle-kit": "^0.19.13"
  }
}
