{
	"name": "@answeroverflow/constants",
	"version": "0.0.0",
	"type": "module",
	"sideEffects": false,
	"exports": {
		"./*": "./src/*.ts"
	},
	"scripts": {
		"typecheck": "tsc --noEmit",
		"clean": "rm -rf .turbo node_modules coverage",
		"with-env": "dotenv -e ../../.env --",
		"format": "prettier --ignore-path ../../.gitignore --config .prettierrc.mjs --write .",
		"format:check": "prettier --ignore-path ../../.gitignore --config .prettierrc.mjs --check .",
		"lint": "eslint . && tsc --noEmit",
		"lint:fix": "eslint . --fix"
	},
	"dependencies": {
		"discord-api-types": "^0.37.54",
		"dotenv-cra": "^3.0.3",
		"zod": "^3.22.2"
	},
	"devDependencies": {
		"@answeroverflow/tsconfig": "workspace:*",
		"@types/node": "^20.11.20",
		"dotenv-cli": "^7.4.1",
		"eslint": "^8.57.0",
		"prettier": "^3.3.3"
	}
}
