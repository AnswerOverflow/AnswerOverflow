		"jest": "^29.6.2",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.3.3",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1",
		"tsc-watch": "^6.0.4",
		"tsup": "^8.1.0",
		"type-fest": "^4.12.0"
	},
	"scripts": {
		"with-env": "dotenv -e ../../.env --",
		"clean": "rm -rf .turbo node_modules dist coverage",
		"watch": "pnpm with-env tsup  --silent --format cjs --watch . --watch ../../packages/api/src/",
		"format": "prettier --ignore-path ../../.gitignore --config .prettierrc.mjs --write .",
		"format:check": "prettier --ignore-path ../../.gitignore --config .prettierrc.mjs --check .",
		"lint": "eslint . && tsc --noEmit",
		"lint:fix": "eslint . --fix",
		"build-deps": "(cd ../../packages/discordjs-mock && pnpm build)",
		"build": "pnpm build-deps && pnpm with-env tsup --format cjs",
		"dev": "pnpm build-deps && pnpm with-env rm -rf dist && pnpm build && run-p watch start",
		"start": "pnpm with-env node dist/index.js",
		"test": "pnpm build-deps && pnpm with-env jest --coverage",
		"test:watch": "pnpm with-env jest --watch",
		"install-local-discordjs-react": "cp -r ~/repos/discordjs-react/packages/discordjs-react/dist/ /AnswerOverflow/node_modules/@answeroverflow/discordjs-react/"
	}
}
