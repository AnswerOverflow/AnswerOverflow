{
	"name": "@answeroverflow/main-site",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"with-env": "dotenv -e ../../.env --",
		"dev-next": "pnpm with-env next dev",
		"dev-proxy": "ts-node --project tsconfig-tsnode.json scripts/proxy.ts",
		"dev": "pnpm concurrently \"pnpm dev-next\" \"pnpm dev-proxy\"",
		"build": "pnpm with-env next build",
		"clean": "rm -rf .next .turbo node_modules",
		"start": "pnpm with-env next start",
		"format": "prettier --ignore-path ../../.gitignore --config .prettierrc.mjs --write .",
		"format:check": "prettier --ignore-path ../../.gitignore --config .prettierrc.mjs --check .",
		"lint": "rm -rf .next && pnpm with-env next lint && tsc --noEmit",
		"lint:fix": "pnpm with-env next lint --fix",
		"type-check": "tsc --noEmit"
	},
	"dependencies": {
		"@answeroverflow/analytics": "workspace:^",
		"@answeroverflow/api": "workspace:^",
		"@answeroverflow/auth": "workspace:^",
		"@answeroverflow/cache": "workspace:^",
		"@answeroverflow/constants": "workspace:^",
		"@answeroverflow/db": "workspace:^",
		"@answeroverflow/env": "workspace:^",
		"@answeroverflow/tailwind-config": "workspace:^",
		"@answeroverflow/ui": "workspace:^",
		"@answeroverflow/utils": "workspace:^",
		"@aws-sdk/client-s3": "^3.577.0",
		"@mdx-js/loader": "^2.3.0",
		"@mdx-js/react": "^2.3.0",
		"@next/mdx": "15.0.0-rc.0",
		"@opentelemetry/exporter-trace-otlp-http": "^0.52.0",
		"@opentelemetry/sdk-trace-node": "^1.25.0",
		"@tailwindcss/typography": "^0.5.10",
		"@trpc/server": "^10.45.2",
		"@vercel/otel": "^1.8.3",
		"bright": "^0.8.4",
		"mdx": "^0.3.1",
		"next": "15.0.0-rc.0",
		"next-auth": "^4.23.1",
		"next-axiom": "^1.3.0",
		"next-mdx-remote": "^5.0.0",
		"next-themes": "^0.2.1",
		"react": "19.0.0-rc-6230622a1a-20240610",
		"react-dom": "19.0.0-rc-6230622a1a-20240610",
		"react-fast-marquee": "^1.6.0",
		"react-icons": "^4.10.1",
		"react-schemaorg": "^2.0.0",
		"react-social-icons": "^6.14.0",
		"react-xarrows": "^2.0.2",
		"schema-dts": "^1.1.2",
		"sharp": "^0.32.5",
		"zod": "^3.22.2",
		"dotenv-cli": "^7.4.1"
	},
	"devDependencies": {
		"@answeroverflow/eslint-config-custom": "workspace:^",
		"@answeroverflow/prettier-config": "workspace:^",
		"@answeroverflow/tsconfig": "workspace:^",
		"@edge-runtime/vm": "^2.1.2",
		"@types/http-proxy": "^1.17.11",
		"@types/mdx": "^2.0.13",
		"@types/node": "^20.11.20",
		"@types/react": "^18.2.20",
		"@types/react-dom": "^18.2.7",
		"@vitest/coverage-v8": "^2.0.5",
		"autoprefixer": "^10.4.15",
		"concurrently": "^8.2.2",
		"eslint": "^8.57.0",
		"eslint-config-next": "14.1.3",
		"http-proxy": "^1.18.1",
		"postcss": "^8.4.28",
		"prettier": "^3.3.3",
		"sass": "^1.66.1",
		"tailwindcss": "^3.3.3",
		"ts-node": "^10.9.1",
		"vitest": "^2.0.5"
	}
}
