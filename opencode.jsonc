		// 	"command": [
		// 		"npx",
		// 		"-y",
		// 		"chrome-devtools-mcp@latest"
		// 	]
		// }
	},
	"tools": {
		"posthog_*": false,
		"axiom_*": false,
		"sentry_*": false,
		"vercel_*": false,
		"kernel_*": false
	}
}
